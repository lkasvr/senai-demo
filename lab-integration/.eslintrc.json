{
  "parser": "@typescript-eslint/parser",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["@stylistic/js", "@typescript-eslint"],
  "rules": {
    "@stylistic/js/semi": ["error", "always"]
  }
}
